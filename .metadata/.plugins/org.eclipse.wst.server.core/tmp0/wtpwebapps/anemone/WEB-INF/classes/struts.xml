<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.ui.theme" value="simple" />
<!-- 開発者モード(デバッグしたい時にはコメント外してください。ユーザーのエラーページが無効にできます) -->
	<constant name="struts.devMode" value="true" />
	<package name="com.internousdev.anemone.action" extends="struts-default">

	<action name="AddCartAction" class="com.internousdev.anemone.action.AddCartAction" method="execute">
		<result name="success">cart.jsp</result>
		<result name="error">error.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="CartAction" class="com.internousdev.anemone.action.CartAction" method="execute">
		<result name="success">cart.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="CreateDestinationAction" class="com.internousdev.anemone.action.CreateDestinationAction" method="execute">
		<result name="success">createDestination.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="CreateDestinationCompleteAction" class="com.internousdev.anemone.action.CreateDestinationCompleteAction" method="execute">
		<result name="success">createDestinationComplete.jsp</result>
		<result name="error">error.jsp</result>
		<result name="inputError">createDestination.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="CreateDestinationConfirmAction" class="com.internousdev.anemone.action.CreateDestinationConfirmAction" method="execute">
		<result name="success">createDestinationConfirm.jsp</result>
		<result name="error">createDestination.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="CreateUserAction" class="com.internousdev.anemone.action.CreateUserAction" method="execute">
		<result name="success">createUser.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

    <action name="CreateUserCompleteAction" class="com.internousdev.anemone.action.CreateUserCompleteAction" method="execute">
		<result name="success">createUserComplete.jsp</result>
		<result name="error">error.jsp</result>
		<result name="inputError">createUser.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

    <action name="CreateUserConfirmAction" class="com.internousdev.anemone.action.CreateUserConfirmAction" method="execute">
		<result name="success">createUserConfirm.jsp</result>
		<result name="error">createUser.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

  	<action name="DeleteCartAction" class="com.internousdev.anemone.action.DeleteCartAction" method="execute">
		<result name="success">cart.jsp</result>
		<result name="error">error.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

  	<action name="DeletePurchaseHistoryAction" class="com.internousdev.anemone.action.DeletePurchaseHistoryAction" method="execute">
		<result name="success">purchaseHistory.jsp</result>
		<result name="error">error.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

 	<action name="GoLoginAction" class="com.internousdev.anemone.action.GoLoginAction" method="execute">
		<result name="success">login.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

   	<action name="HomeAction" class="com.internousdev.anemone.action.HomeAction" method="execute">
		<result name="success">home.jsp</result>
	</action>

    <action name="LoginAction" class="com.internousdev.anemone.action.LoginAction" method="execute">
		<result name="success">home.jsp</result>
		<result name="cart">cart.jsp</result>
		<result name="error">login.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="LogoutAction" class="com.internousdev.anemone.action.LogoutAction" method="execute">
		<result name="success">index.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="MyPageAction" class="com.internousdev.anemone.action.MyPageAction" method="execute">
		<result name="success">mypage.jsp</result>
		<result name="error">error.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

   	<action name="ProductDetailsAction" class="com.internousdev.anemone.action.ProductDetailsAction" method="execute">
		<result name="success">productDetails.jsp</result>
		<result name="error">error.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

   	<action name="ProductListAction" class="com.internousdev.anemone.action.ProductListAction" method="execute">
		<result name="success">productList.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="PurchaseHistoryAction" class="com.internousdev.anemone.action.PurchaseHistoryAction" method="execute">
		<result name="success">purchaseHistory.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="ResetPasswordAction" class="com.internousdev.anemone.action.ResetPasswordAction" method="execute">
		<result name="success">resetPassword.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="ResetPasswordCompleteAction" class="com.internousdev.anemone.action.ResetPasswordCompleteAction" method="execute">
		<result name="success">resetPasswordComplete.jsp</result>
		<result name="error">error.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

	<action name="ResetPasswordConfirmAction" class="com.internousdev.anemone.action.ResetPasswordConfirmAction" method="execute">
		<result name="success">resetPasswordConfirm.jsp</result>
		<result name="error">resetPassword.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

   	<action name="SearchItemAction" class="com.internousdev.anemone.action.SearchItemAction" method="execute">
		<result name="success">productList.jsp</result>
		<result name="error">productList.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

   	<action name="SettlementCompleteAction" class="com.internousdev.anemone.action.SettlementCompleteAction" method="execute">
		<result name="success">settlementComplete.jsp</result>
		<result name="error">error.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

   	<action name="SettlementConfirmAction" class="com.internousdev.anemone.action.SettlementConfirmAction" method="execute">
		<result name="success">settlementConfirm.jsp</result>
		<result name="login">login.jsp</result>
		<result name="sessionError">error.jsp</result>
	</action>

</package>
</struts>
